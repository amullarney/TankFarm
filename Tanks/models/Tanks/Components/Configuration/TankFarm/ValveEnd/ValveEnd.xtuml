INSERT INTO O_OBJ
	VALUES ("1d5ab4c5-7383-4769-aedb-d550e0681969",
	'ValveEnd',
	5,
	'ValveEnd',
	'Each valve has two ''ends''; each end connects either to a tank or to a pipe end.
Valves are non-directional: the ends cannot be considered to be inlet or outlet.
Flow paths can be developed in any direction.',
	"00000000-0000-0000-0000-000000000000");
INSERT INTO O_TFR
	VALUES ("2967108e-ad84-4af8-b368-3a9845eac0d5",
	"1d5ab4c5-7383-4769-aedb-d550e0681969",
	'AddValveToPath',
	'',
	"ba5eda7a-def5-0000-0000-000000000000",
	1,
	'  select one valve related by self->Valve[R1.''is one side of''];
  if ( empty valve )
    select one valve related by self->Valve[R1.''is other side of''];
  end if;
  select any path from instances of FlowPath; 
  relate valve to path across R6;
',
	1,
	'',
	"00000000-0000-0000-0000-000000000000",
	0);
INSERT INTO O_TFR
	VALUES ("5880c536-b954-45c4-947c-61789e0bd08d",
	"1d5ab4c5-7383-4769-aedb-d550e0681969",
	'FindTank',
	'Seek a path connecting to the named destination tank.',
	"ba5eda7a-def5-0000-0000-000000000001",
	1,
	'// Find the other side of this valve - do not know which way to traverse R1
success = false;
self.seen = true;
select one valveEnd related by self->ValveEnd[R1.''is one side of''];
if ( empty valveEnd )
  select one valveEnd related by self->ValveEnd[R1.''is other side of''];
end if;
valveEnd.seen = true;
select one tank related by valveEnd->Tank[R3.''acesses''];  // connected to a tank?
if ( not_empty tank )
  if ( tank.TankIdentifier == param.Destination )
    success = true;
  end if;
else
  select one pipeEnd related by valveEnd->PipeEnd[R2.''is attached to''];  // maybe a pipe?
  if ( not_empty pipeEnd )
    success = pipeEnd.FindPath( Destination:param.Destination );
  end if;
end if;
if ( success )
  self.AddValveToPath();
end if;
return success;',
	1,
	'',
	"2967108e-ad84-4af8-b368-3a9845eac0d5",
	0);
